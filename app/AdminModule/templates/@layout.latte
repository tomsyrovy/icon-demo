					</nav> <!-- .nav -->

				</div> <!-- .col-12 -->
			</div> <!-- .row -->
		</div> <!-- .container -->





		<!-- ========== Header ========== -->
		<div class="container">
			<div class="row">
				<div class="col-12">
				
					<header class="header" role="banner">
						<h1 class="page-title">
							<a title="Přejít na úvodní stranu" n:href="Homepage:default">
								<img src="{$basePath}/assets/images/logo.png" alt="iCON"></a> {block pageTitle}Administrace{/block}
						</h1>
					</header>
				
				</div> <!-- .col-12 -->
			</div> <!-- .row -->
		</div> <!-- .container -->	





		<!-- ========== Content ========== -->
		<div class="container container-main" role="main">
			<div class="row">
		
				<div n:foreach="$flashes as $flash" class="flash {$flash->type}">{$flash->message}</div>

				{block content}{/block}
			
			</div> <!-- .wrap_ins -->
		</div> <!-- .wrap -->	





		<!-- ========== Footer ========== -->
		<div class="container">
			<div class="row">	
				<div class="col-12">

					<footer class="footer" role="contentinfo">
						<div class="footer-copy">
							<div class="row">
								<div class="col-6">
									Copyright &copy; 2015, iLab.
								</div> <!-- .col-6 -->

								<div class="col-6 text-right">
									<a class="logo-query" title="Tvorba internetových stránek a on-line marketing QUERY" href="http://www.query.cz"><img src="{$basePath}/assets/images/transparent.gif" alt="Query"></a>
									<a class="logo-eternia" title="Grafické studio Eternia" href="http://www.eternia.cz"><img src="{$basePath}/assets/images/transparent.gif" alt="Query"></a>
								</div> <!-- .col-6 -->
							</div> <!-- .row -->
