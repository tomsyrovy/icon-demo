		<script type="text/javascript" src="{$basePath}/assets/js/main.min.js?26a69489"></script>
		{block script}{/block}

		<script type="text/javascript" n:syntax='off'>
			tinymce.init({
				selector: ".tinymce",
				entity_encoding : "raw",
				plugins: [
					"advlist autolink lists link image charmap print preview hr anchor pagebreak",
					"searchreplace wordcount visualblocks visualchars code fullscreen",
					"insertdatetime media nonbreaking save table contextmenu directionality",
					"emoticons template paste textcolor colorpicker textpattern"
				],
				toolbar1: "styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent",
				toolbar2: "link unlink anchor | removeformat | visualblocks nonbreaking | code",
				menubar: false,
				toolbar_items_size: 'small'
			});
		</script>





