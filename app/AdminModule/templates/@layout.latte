	<body class="{block bodyClass}{/block}">





		<!-- ========== Mobile nav ========== -->
		<nav class="nav-mobile" role="navigation">
			<ul>
				{block #nav}
				<li{if $presenter->isLinkCurrent("TalkType:*")} class="active"{/if}>
					<a n:href="TalkType:default">Úvodní stránka</a>
				</li>
				{*<li{if $presenter->isLinkCurrent("Text:*")} class="active"{/if}>*}
					{*<a n:href="Text:default">Texty webu</a>*}
				{*</li>*}
				<li{if $presenter->isLinkCurrent("Headline:*")} class="active"{/if}>
					<a n:href="Headline:default">Předprogram</a>
				</li>
				<li{if $presenter->isLinkCurrent("Talk:*")} class="active"{/if}><a title="Program" n:href="Talk:default">Program</a>
					<ul>
						<li><a n:href="Talk:default">Správa programu</a></li>
						<li><a n:href="Talk:description">Popis iCON bloků</a></li>
						<li><a href="">Vlastní program - analýza</a></li>
					</ul>
				</li>
				<li{if $presenter->isLinkCurrent("Speaker:*")} class="active"{/if}>
					<a n:href="Speaker:default">Řečníci</a>
				</li>
				<li{if $presenter->isLinkCurrent("Blog:*")} class="active"{/if}><a title="iCONčlánky (festival)" n:href="Blog:default">iCONčlánky (festival)</a>
					<ul>
						<li><a n:href="Blog:default">Správa článků</a></li>
						<li><a n:href="Blog:images">Správa obrázků</a></li>
					</ul>
				</li>
				<li{if $presenter->isLinkCurrent("Feed:*")} class="active"{/if}>
					<a n:href="Feed:default" target="_blank">Generování iOS app feed</a>
				</li>
				{/block}
			</ul>
		</nav> <!-- .nav-mobile -->





		<!-- ========== Main nav ========== -->
		<div class="container">
			<div class="row">
				<div class="col-12">
					
					<nav class="nav" role="navigation">


